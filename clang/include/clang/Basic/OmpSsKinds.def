#endif
#ifndef OMPSS_DECLARE_TASK_CLAUSE
#  define OMPSS_DECLARE_TASK_CLAUSE(Name)
#endif
#ifndef OMPSS_DEFAULT_KIND
#  define OMPSS_DEFAULT_KIND(Name)
#endif
#ifndef OMPSS_DEPEND_KIND
#define OMPSS_DEPEND_KIND(Name)
#endif
#ifndef OMPSS_CLAUSE_ALIAS
#define OMPSS_CLAUSE_ALIAS(Alias, Name)
#endif

// OmpSs directives.
OMPSS_DIRECTIVE(task)
OMPSS_DIRECTIVE(taskloop)
OMPSS_DIRECTIVE(taskwait)
OMPSS_DIRECTIVE(release)
OMPSS_DIRECTIVE(assert)

// OmpSs clauses.
OMPSS_CLAUSE(if, OSSIfClause)
OMPSS_CLAUSE(final, OSSFinalClause)
OMPSS_CLAUSE(cost, OSSCostClause)
OMPSS_CLAUSE(priority, OSSPriorityClause)
OMPSS_CLAUSE(label, OSSLabelClause)
OMPSS_CLAUSE(wait, OSSWaitClause)
OMPSS_CLAUSE(onready, OSSOnreadyClause)
OMPSS_CLAUSE(default, OSSDefaultClause)
OMPSS_CLAUSE(private, OSSPrivateClause)
OMPSS_CLAUSE(firstprivate, OSSFirstprivateClause)
OMPSS_CLAUSE(shared,  OSSSharedClause)
OMPSS_CLAUSE(depend, OSSDependClause)
OMPSS_CLAUSE(reduction, OSSReductionClause)
OMPSS_CLAUSE(chunksize, OSSChunksizeClause)
OMPSS_CLAUSE(grainsize, OSSGrainsizeClause)
OMPSS_CLAUSE(collapse,  OSSCollapseClause)
OMPSS_DIRECTIVE_EXT(declare_reduction, "declare reduction")
OMPSS_DIRECTIVE_EXT(declare_task, "task")
OMPSS_DIRECTIVE_EXT(task_for, "task for")
OMPSS_DIRECTIVE_EXT(taskloop_for, "taskloop for")

// Static attributes for 'default' clause.
OMPSS_DEFAULT_KIND(none)
OMPSS_DEFAULT_KIND(shared)

// Static attributes for 'depend' clause.
OMPSS_DEPEND_KIND(in)
OMPSS_DEPEND_KIND(out)
OMPSS_DEPEND_KIND(inout)
// Concurrent
OMPSS_DEPEND_KIND(inoutset)
// Commutative
OMPSS_DEPEND_KIND(mutexinoutset)
OMPSS_DEPEND_KIND(weak)

// OmpSs-2 'depend' syntax aliases
OMPSS_CLAUSE_ALIAS(in, depend)
OMPSS_CLAUSE_ALIAS(out, depend)
OMPSS_CLAUSE_ALIAS(inout, depend)
OMPSS_CLAUSE_ALIAS(concurrent, depend)
OMPSS_CLAUSE_ALIAS(commutative, depend)
OMPSS_CLAUSE_ALIAS(weakin, depend)
OMPSS_CLAUSE_ALIAS(weakout, depend)
OMPSS_CLAUSE_ALIAS(weakinout, depend)
OMPSS_CLAUSE_ALIAS(weakconcurrent, depend)
OMPSS_CLAUSE_ALIAS(weakcommutative, depend)
// OmpSs-2 'reduction' alias
OMPSS_CLAUSE_ALIAS(weakreduction, reduction)


# RUN: llc -o - -mtriple=riscv64-unknown-linux-gnu -mattr=+epi \
# RUN:     -verify-machineinstrs %s | FileCheck %s
---
name:            test
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $v2, $x10
    $v1 = COPY $v2
    VSE_V $v1, $x10, implicit $vl, implicit $vtype
...

# CHECK: rdvtype t0
# CHECK-NEXT: rdvl t1
# CHECK-NEXT: vsetvli zero, zero, e64, m1
# CHECK-NEXT: vadd.vi v1, v2, 0
# CHECK-NEXT: vsetvl  zero, t1, t0
# CHECK-NEXT: vse.v v1, (a0)
